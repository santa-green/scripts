<TfrxReport Version="4.12" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="40862,573184919" ReportOptions.Description.Text="" ReportOptions.LastChange="43741,7380718403" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  RegPath: String = 'Бизнес\Отчёты\Доп соглашение';&#13;&#10;  CName: String;&#13;&#10;  CopyNum: Integer;                           &#13;&#10;&#13;&#10;procedure btOkOnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  AFilter: String;&#13;&#10;begin&#13;&#10;  if (cb_Ours.Keyvalue = null) or ((cb_CompGrs2.Keyvalue = null) and (Trim(eComps.Text) = '')) or (cb_Emps.Keyvalue = null) then&#13;&#10;  begin&#13;&#10;    frmParams.ModalResult := mrNone;&#13;&#10;    MessageDlg('Если не знаете, с чего начать, заполните сперва поля &#34;Фирма&#34;, &#34;Группа предприятий&#34; и &#34;Представитель фирмы&#34;. Благодарим за внимание.', mtInformation, mbOk, 0);&#13;&#10;    Exit;&#13;&#10;  end;&#13;&#10;&#13;&#10;  Реестр_Записать('OurID', VarToStr(cb_Ours.KeyValue), RegPath); //фирма              &#13;&#10;  Реестр_Записать('CompGrID2', VarToStr(cb_CompGrs2.KeyValue), RegPath); //группа предприятий 2&#13;&#10;  Реестр_Записать('CompType', VarToStr(cb_CompTypes.KeyValue), RegPath);   //тип предприятия      &#13;&#10;  Реестр_Записать('eComps', eComps.Text, RegPath); //предприятия&#13;&#10;  Реестр_Записать('SRID_Name', VarToStr(cbSRIDs.KeyValue), RegPath);  //торговый представитель&#13;&#10;  Реестр_Записать('EmpName', VarToStr(cb_Emps.KeyValue), RegPath);  //представитель фирмы&#13;&#10;                                                                        &#13;&#10;  dq_Doc.Open;&#13;&#10;&#13;&#10;  if dq_Doc.IsEmpty then&#13;&#10;  begin&#13;&#10;    dq_Doc.Close;&#13;&#10;    frmParams.ModalResult := mrNone;&#13;&#10;    MessageDlg('Нет данных для печати, проверьте установленные фильтры.', mtError, mbOk, 0);&#13;&#10;    Exit;&#13;&#10;  end;&#13;&#10; &#13;&#10;end;&#13;&#10;&#13;&#10;procedure sbCompsOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10; { eComps.Text := выборспискаизсправочника('(SELECT CompID, CAST(CompID AS VARCHAR(25))+ CHAR(32) +' +&#13;&#10;    ' CHAR(45) + CHAR(32) + CompName CompName FROM r_Comps m WITH(NOLOCK) WHERE (CompGrID2 = ' + &#13;&#10;    dq_CompGrs2.Fields[0].AsString + ' OR ' + VarToStr(IIF(cb_CompGrs2.KeyValue = null, 1, 0)) +  ' = 1) ' +&#13;&#10;    ' AND CompType = 1' + &#13;&#10;    ' AND EXISTS(SELECT * FROM dbo.at_z_Contracts WITH(NOLOCK) WHERE CompID=m.CompID AND ContrTypeID=1 AND Status=1 AND OurID=' + VarToStr(cb_Ours.KeyValue) + ')) m ORDER BY 1--;CompID;CompName;'); }  &#13;&#10;&#13;&#10; eComps.Text:=ВыборСпискаИзСправочника('r_Comps;CompID;CompName;'+eComps.Text);  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure frmParamsOnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  cb_Ours.keyvalue := Реестр_Прочитать('OurID', VarToStr(БДФункция('zf_GetOurID','t')), RegPath); //фирма     &#13;&#10;  cb_CompGrs2.KeyValue := Реестр_Прочитать('CompGrID2', '', RegPath);  //группа предприятий 2&#13;&#10;  cb_CompTypes.KeyValue := Реестр_Прочитать('CompType', '', RegPath);  //тип предприятия      &#13;&#10;  eComps.Text := Реестр_Прочитать('eComps', '', RegPath);     //предприятия        &#13;&#10;  cbSRIDs.KeyValue := Реестр_Прочитать('SRID_Name','',RegPath);        //торговый представитель  &#13;&#10;  cb_Emps.KeyValue := Реестр_Прочитать('EmpName', '', RegPath);        //представитель фирмы                                                                                                              &#13;&#10;    &#13;&#10;end;&#13;&#10;&#13;&#10;procedure cb_OursOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page1OnManualBuild(Sender: TfrxComponent);&#13;&#10;var  &#13;&#10;i: Integer;    &#13;&#10;  &#13;&#10;begin&#13;&#10; dq_Doc.Open;&#13;&#10;	dq_Doc.First;  &#13;&#10;  &#13;&#10;		while not dq_Doc.Eof do      &#13;&#10;			begin              &#13;&#10;				with Engine do&#13;&#10;				begin&#13;&#10;       CopyNum := 1;&#13;&#10;       ShowBand(Header1);                                                        &#13;&#10;				   ShowBand(MasterData1);&#13;&#10;       NewPage;                             &#13;&#10;				   CopyNum := 2;&#13;&#10;       ShowBand(Header1);    &#13;&#10;       ShowBand(MasterData1);&#13;&#10;       ShowBand(Child4);                                                            &#13;&#10;       NewPage;                                      &#13;&#10;				end;&#13;&#10;				dq_Doc.Next;  			&#13;&#10;			end;&#13;&#10;	dq_Doc.Close;	  &#13;&#10;end;&#13;&#10;&#13;&#10;{*&#13;&#10;procedure Page2OnManualBuild(Sender: TfrxComponent);&#13;&#10;var  &#13;&#10;i: Integer;    &#13;&#10;  &#13;&#10;begin&#13;&#10;      &#13;&#10; dq_Doc.Open;&#13;&#10;	dq_Doc.First;  &#13;&#10;  &#13;&#10;		while not dq_Doc.Eof do      &#13;&#10;			begin              &#13;&#10;				with Engine do&#13;&#10;				begin   &#13;&#10;       ShowBand(Child4);                                      &#13;&#10;				end;&#13;&#10;				dq_Doc.Next;  			&#13;&#10;			end;&#13;&#10;	dq_Doc.Close;	  &#13;&#10;end;&#13;&#10;*}        &#13;&#10;  &#13;&#10;begin&#13;&#10;&#13;&#10;end." StoreInDFM="False" PropData="">
  <TfrxDataPage Name="Data" Height="1000" Left="0" Top="0" Width="1000">
    <TfrxBDEQuery Name="dq_Doc" UserName="dq_Doc" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="True" SQL.Text="SELECT DISTINCT rc.CompID, rc.Code, rc.TaxPayer,&#13;&#10;RTRIM(REPLACE(REPLACE(REPLACE(REPLACE(rc.Address + ISNULL(', ' + NULLIF(rc.City,''), '') + ISNULL(', ' + NULLIF(rc.Region,''), '') + ISNULL(', ' + NULLIF(rc.PostIndex,''), ''), '  ', ' '), ', ', ','), ' ,', ','), ',', ', ')) Address,&#13;&#10;RTRIM(CASE WHEN ISNULL(rc.Phone1, '') != '' AND ISNULL(rc.Phone2, '') != '' THEN rc.Phone1 + ', ' + rc.Phone2&#13;&#10;WHEN ISNULL(rc.Phone1, '') != '' THEN rc.Phone1&#13;&#10;WHEN ISNULL(rc.Phone2, '') != '' THEN rc.Phone2 END) Phone, LTRIM(RTRIM(rc.Contract1)) Contract1,&#13;&#10;LTRIM(RTRIM(CASE WHEN NULLIF(rc.Contract2, '') IS NOT NULL AND NULLIF(rc.Contract3, '') IS NOT NULL THEN rc.Contract1 + '; ' + rc.Contract2 ELSE rc.Contract1 END)) FullCompName,&#13;&#10;con.ContrID, con.BDate, rc.CompName                                       &#13;&#10;FROM dbo.r_Comps rc WITH(NOLOCK)&#13;&#10;LEFT JOIN dbo.at_r_CompOurTerms rcot WITH(NOLOCK) ON rcot.CompID = rc.CompID&#13;&#10;JOIN dbo.at_z_Contracts con WITH(NOLOCK) ON con.CompID = rc.CompID AND con.Status = 1 AND con.ContrTypeID = 1 AND con.OurID = rcot.OurID                  &#13;&#10;WHERE (:Comps = '' OR rc.CompID IN (SELECT AValue FROM dbo.af_FilterToTableByTable(:Comps, 'r_Comps')))&#13;&#10;AND rcot.OurID = :OurID&#13;&#10;AND rc.CompType = :CompType&#13;&#10;AND rc.CompGrID2 BETWEEN 2030 AND 2099&#13;&#10;AND (:IsSRID = 1 OR rcot.SRID = :SRID)&#13;&#10;AND (:IsCompGrID2 = 1 OR rc.CompGrID2 = :CompGrID2)&#13;&#10;AND con.EDate &#62; getdate() --rkv0 26.09.2019 фильтруем только действующие договора                                                                                                                                                                                                        &#13;&#10;ORDER BY 1" DatabaseName="dbMain" SessionName="Default" PropData=""/>
    <TfrxBDEQuery Name="dq_CompGrs2" UserName="dq_CompGrs2" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="dq_Ours" IgnoreDupParams="True" SQL.Text="SELECT DISTINCT m.CompGrID2, CAST(CAST(m.CompGrID2 AS VARCHAR(10)) + ' - ' + m.CompGrName2 AS VARCHAR(255)) CompGrName2, :OurID OurID&#13;&#10;FROM r_CompGrs2 m WITH(NOLOCK)&#13;&#10;JOIN r_Comps d WITH(NOLOCK) ON d.CompGrID2 = m.CompGrID2&#13;&#10;WHERE &#13;&#10;(m.CompGrID2 BETWEEN 2030 AND 2099 OR m.CompGrID2 = 3)&#13;&#10;AND EXISTS(SELECT * FROM dbo.at_z_Contracts WITH(NOLOCK) WHERE CompID=d.CompID AND Status=1 AND ContrTypeID=1 AND OurID=:OurID)&#13;&#10;AND d.CompType = :CompType&#13;&#10;--убираем код 2098 из Фирмы 3                                  &#13;&#10;AND NOT (:OurID = 3 AND m.CompGrID2 = 2098)                                                                                                                               &#13;&#10;ORDER BY 1, 2&#13;&#10;    " DatabaseName="dbMain" SessionName="Default" PropData=""/>
    <TfrxBDEQuery Name="dq_Ours" UserName="dq_Ours" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT OurID, CAST(OurID AS VARCHAR(3)) + ' - ' + OurName OurName, Note2 FullOurName, Code&#13;&#10;FROM dbo.r_Ours&#13;&#10;WHERE OurID IN (SELECT AValue FROM dbo.af_GetValidsTable('ValidOurs')) AND OurID IN (1,3)&#13;&#10;ORDER BY 1" DatabaseName="dbMain" SessionName="Default" PropData="05704C65667402340470546F7003B0000A506172616D65746572730100"/>
    <TfrxBDEQuery Name="dq_SrIDs" UserName="dq_SRIDs" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="dq_CompGrs2" IgnoreDupParams="True" SQL.Text="SELECT DISTINCT rsr.SRID, CAST(CAST(rsr.SRID AS VARCHAR(10)) + ' - ' + SRName AS VARCHAR(250)) SRName&#13;&#10;FROM dbo.r_Comps rc WITH(NOLOCK)&#13;&#10;LEFT JOIN dbo.at_r_CompOurTerms rcot WITH(NOLOCK) ON rcot.CompID = rc.CompID&#13;&#10;JOIN dbo.at_r_SRS rsr WITH(NOLOCK) ON rcot.SRID = rsr.SRID&#13;&#10;WHERE (:IsCompGrID2 = 1 OR rc.CompGrID2 = :CompGrID2)&#13;&#10;AND rcot.OurID = :OurID&#13;&#10;AND EXISTS(SELECT * FROM dbo.at_z_Contracts WITH(NOLOCK) WHERE CompID=rc.CompID AND Status=1 AND ContrTypeID=1 AND OurID=:OurID)&#13;&#10;AND rc.CompType = :CompType        &#13;&#10;ORDER BY 1" DatabaseName="dbMain" SessionName="Default" PropData=""/>
    <TfrxBDEQuery Name="dq_CompTypes" UserName="dq_CompTypes" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT DISTINCT CompType, CAST(CompType AS VARCHAR(2)) + ' - ' + CASE CompType WHEN 0 THEN 'Юридическое лицо' WHEN 1 THEN 'Физическое лицо' END AS CompTypeName&#13;&#10;FROM dbo.r_Comps&#13;&#10;ORDER BY 1    " DatabaseName="dbMain" SessionName="Default" PropData="05704C65667403C0000470546F7003E4000A506172616D65746572730100"/>
    <TfrxBDEQuery Name="dq_Emps" UserName="dq_Emps" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="dq_CompGrs2" IgnoreDupParams="True" SQL.Text="IF :OurID = 1                                 &#13;&#10;BEGIN                  &#13;&#10;	SELECT DISTINCT&#13;&#10;	e.EmpID, e.UAEmpName EmpName, oo.AttorID, oo.AttorDate&#13;&#10;	FROM dbo.at_r_OurOfficials oo WITH(NOLOCK)&#13;&#10;	JOIN dbo.r_Emps e WITH(NOLOCK) ON e.EmpID = oo.EmpID&#13;&#10;	WHERE &#13;&#10;	oo.OurID = 1&#13;&#10;	AND oo.CompGrID2 = :CompGrID2 AND e.EmpName NOT LIKE '%Уволен%'  &#13;&#10;	ORDER BY 2&#13;&#10;END&#13;&#10;&#13;&#10;ELSE&#13;&#10;BEGIN&#13;&#10;	SELECT DISTINCT&#13;&#10;	e.EmpID, e.UAEmpName EmpName, oo.AttorID, oo.AttorDate&#13;&#10;	FROM dbo.at_r_OurOfficials oo WITH(NOLOCK)&#13;&#10;	JOIN dbo.r_Emps e WITH(NOLOCK) ON e.EmpID = oo.EmpID&#13;&#10;	WHERE &#13;&#10;	oo.OurID = 3&#13;&#10;	AND oo.CompGrID2 = :CompGrID2 AND e.EmpName NOT LIKE '%Уволен%'  &#13;&#10;	ORDER BY 2&#13;&#10;END        " DatabaseName="dbMain" SessionName="Default" PropData=""/>
    <TfrxBDEQuery Name="dq_req" UserName="dq_req" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="True" SQL.Text="SELECT TaxRegNo = CASE WHEN TaxRegNo = 'х' THEN '-' ELSE TaxRegNo END, * FROM r_Ours m &#13;&#10;JOIN r_OursCC roc WITH (NOLOCK) ON roc.OurID = m.OurID --AND roc.DefaultAccount = 1&#13;&#10;JOIN r_Banks rb WITH (NOLOCK) ON rb.BankID = roc.BankID&#13;&#10;where m.OurID = :OurID AND roc.CompGrID2 = :CompGrID2 AND rb.BankID = 305749 AND AccountCC like 'UA%' --МФО банка Кредит Днепр прописан константой                                                                                                                          " DatabaseName="dbMain" SessionName="Default" PropData=""/>
