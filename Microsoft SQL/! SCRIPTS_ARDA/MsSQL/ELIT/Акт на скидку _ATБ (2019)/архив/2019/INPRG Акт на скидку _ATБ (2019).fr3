<TfrxReport Version="4.12" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="41939,6438989699" ReportOptions.Description.Text="" ReportOptions.LastChange="43865,6153036806" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;       i: int;                   &#13;&#10;  RegPath: String = 'Отчеты\Бизнес\Акт на скидку';&#13;&#10;  MonthNames: array [0..11] of string = ['січні', 'лютому', 'березні', 'квітні', 'травні', 'червні', 'липні', 'серпні', 'вересні', 'жовтні', 'листопаді', 'грудні'];                                                                                           &#13;&#10;      &#13;&#10;procedure btnOkOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;//InputBox('','',dq_Doc.SQL.Text);      &#13;&#10;i:=0;            &#13;&#10;  if (cbAccount.Text = '') or (edDocNum.Text = '') then&#13;&#10;  begin&#13;&#10;    DialogPage1.ModalResult := mrNone;                                                   &#13;&#10;    MessageDlg('Необходимо заполнить вся обязательные поля.', mtError, mbOk, 0);                          &#13;&#10;  end&#13;&#10;  else&#13;&#10;  begin&#13;&#10;    Реестр_Записать(RegPath, VarToStr(cbAccount.KeyValue), 'cbAccount');&#13;&#10;    Реестр_Записать(RegPath, edDocNum.Text, 'edDocNum');&#13;&#10;  end;                  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure edDocNumOnChange(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  edDocNum.Text := ПараметрИзСтрокиПоШаблону(0, ReplaceStr(edDocNum.Text,'''', ''), '\d+');      &#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;    inc(i);              &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  cbDates.DataSet.Open;&#13;&#10;  cbDates.DataSet.DataSet.Last;&#13;&#10;  cbDates.KeyValue := cbDates.DataSet.DataSet.FieldByName(cbDates.KeyField).Value;&#13;&#10;  edDocNum.Text := Реестр_Прочитать(RegPath, '0', 'edDocNum');        &#13;&#10;&#13;&#10;  cbAccount.KeyValue := Реестр_Прочитать(RegPath, '0', 'cbAccount');                                                &#13;&#10;end." StoreInDFM="False" PropData="">
