<TfrxReport Version="4.12" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="38394,3831604398" ReportOptions.Description.Text="" ReportOptions.LastChange="43880,6856971412" ScriptLanguage="PascalScript" ScriptText.Text="Var&#13;&#10;  RegPath: String = 'Отчеты\Бизнес\Акт приема-передачи';&#13;&#10;&#13;&#10;Procedure RefreshEmps(Sender: TfrxComponent);&#13;&#10;Var&#13;&#10;  TmpEmp: Integer;&#13;&#10;Begin&#13;&#10;  TmpEmp := 0;&#13;&#10;  If Not cbEmpMode.Checked Then&#13;&#10;  Begin&#13;&#10;    If dq_Emps.Active And Not dq_Emps.IsEmpty Then&#13;&#10;      TmpEmp := cbEmps.KeyValue;&#13;&#10;    dq_Emps.Close;&#13;&#10;    dq_Emps.Open;&#13;&#10;    If dq_Emps.Active And Not dq_Emps.IsEmpty Then&#13;&#10;      If dq_Emps.Dataset.Locate('EmpID', TmpEmp, loCaseInsensitive) Then&#13;&#10;        cbEmps.KeyValue := TmpEmp&#13;&#10;      Else&#13;&#10;        cbEmps.KeyValue := dq_Emps.Fields[0].Value;&#13;&#10;  End&#13;&#10;  Else&#13;&#10;  Begin&#13;&#10;    If dq_Emps.Active Then dq_Emps.Close;&#13;&#10;    dq_Emps.Open;&#13;&#10;    dq_Emps.Dataset.Locate('AttorID', 0, loCaseInsensitive);&#13;&#10;    cbEmps.KeyValue := dq_Emps.Fields[0].Value;&#13;&#10;  End;&#13;&#10;End;&#13;&#10;&#13;&#10;procedure frmParamsOnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  dq_Ours.Open;&#13;&#10;  dq_Emps.Open;&#13;&#10;  edtKurs.Text := FloatToStr(БДПоиск('dbo.zf_GetRateMC(dbo.zf_GetCurrCC())', '', ''));&#13;&#10;  deDocDate.Date := БДПоиск('GETDATE()', '', '');&#13;&#10;&#13;&#10;  cbOurID.KeyValue := StrToInt(Реестр_Прочитать('cbOurID',VarToStr(БДФункция('zf_GetOurID', 't')),RegPath));&#13;&#10;  rbPmDoc.Checked := StrToBool(Реестр_Прочитать('rbPmDoc', '1', RegPath));&#13;&#10;  rbPmPrice.Checked := Not StrToBool(Реестр_Прочитать('rbPmDoc', '1', RegPath));&#13;&#10;  cbPLs.KeyValue := StrToInt(Реестр_Прочитать('cbPLs', '0', RegPath));&#13;&#10;  rbKmDocKurs.Checked := StrToBool(Реестр_Прочитать('rbKmDocKurs', '1', RegPath));&#13;&#10;  rbKmSelKurs.Checked := Not StrToBool(Реестр_Прочитать('rbKmDocKurs', '1', RegPath));&#13;&#10;  rbDmDocDate.Checked := StrToBool(Реестр_Прочитать('rbDmDocDate', '1', RegPath));&#13;&#10;  rbDmSelDate.Checked := Not StrToBool(Реестр_Прочитать('rbDmDocDate', '1', RegPath));&#13;&#10;  cbEmpMode.Checked := StrToBool(Реестр_Прочитать('cbEmpMode', '0', RegPath));&#13;&#10;  cbEmps.KeyValue := StrToInt(Реестр_Прочитать('cbEmps', '0', RegPath));&#13;&#10;  rbDocTran.Checked := StrToBool(Реестр_Прочитать('rbDocTran', '1', RegPath));&#13;&#10;  rbDocRec.Checked := Not StrToBool(Реестр_Прочитать('rbDocTran', '1', RegPath));&#13;&#10;  chbDatePrint.Checked := StrToBool(Реестр_Прочитать('chbDatePrint','0',RegPath));&#13;&#10;  rbDmDocDateOnClick(Sender);&#13;&#10;  rbDocRecOnClick(Sender);&#13;&#10;  chbAccOnClick(Sender);&#13;&#10;  rbDocTranOnClick(Sender);&#13;&#10;  chbExcOnClick(Sender);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure rbPmPriceOnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  PrState: Boolean;&#13;&#10;begin&#13;&#10;  PrState := rbPmPrice.Checked;&#13;&#10;  cbPls.Enabled := PrState;&#13;&#10;  rbKmDocKurs.Enabled := PrState;&#13;&#10;  rbKmSelKurs.Enabled := PrState;&#13;&#10;  edtKurs.Enabled := PrState;&#13;&#10;  gbKursMode.Enabled := PrState;&#13;&#10;  rbKmDocKursOnClick(Sender);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure rbKmDocKursOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  edtKurs.Enabled := rbKmSelKurs.Checked And gbKursMode.Enabled;&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure rbDmDocDateOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  deDocDate.Enabled := rbDmSelDate.Checked And Not chbDatePrint.Checked;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure cbEmpModeOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  cbEmps.Enabled := Not cbEmpMode.Checked;&#13;&#10;  RefreshEmps(Sender);&#13;&#10;&#13;&#10;{  If dq_Emps.Filtered Then dq_Emps.Filtered := False;&#13;&#10;&#13;&#10;  If TfrxCheckBoxControl(Sender).Checked Then&#13;&#10;    dq_Emps.Filter := 'AttorID = 0'&#13;&#10;  Else&#13;&#10;    dq_Emps.Filter := 'AttorID &#60;&#62; 0';&#13;&#10;  dq_Emps.Filtered := True;  }&#13;&#10;end;&#13;&#10;&#13;&#10;procedure edtKursOnKeyPress(Sender: TfrxComponent; var Key: Char);&#13;&#10;begin&#13;&#10;  If Not (Key In ['0'..'9',#8,#13,',','.']) Then Key:= #0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure bntOkOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  RefreshEmps(Sender);&#13;&#10;  if (edtDocID.Text = '') Or (cbOurID.Text = '')&#13;&#10;    Or ((cbPls.Text = '') And cbPls.Enabled)&#13;&#10;    Or ((edtKurs.Text = '') And edtKurs.Enabled)&#13;&#10;    Or (cbEmps.Text = '')&#13;&#10;   then&#13;&#10;  begin&#13;&#10;    frmParams.ModalResult := 0;&#13;&#10;    RaiseException('Все предлагаемые поля обязательны для заполнения!');&#13;&#10;  end&#13;&#10;  Else&#13;&#10;  Begin&#13;&#10;  // сохраняем настройки&#13;&#10;    Реестр_Записать('cbOurID',VarToStr(cbOurID.KeyValue),RegPath);&#13;&#10;    Реестр_Записать('cbPLs',VarToStr(cbPLs.KeyValue),RegPath);&#13;&#10;    Реестр_Записать('cbEmps',VarToStr(cbEmps.KeyValue),RegPath);&#13;&#10;    Реестр_Записать('rbPmDoc',BoolToStr(rbPmDoc.Checked),RegPath);&#13;&#10;    Реестр_Записать('rbKmDocKurs',BoolToStr(rbKmDocKurs.Checked),RegPath);&#13;&#10;    Реестр_Записать('rbDmDocDate',BoolToStr(rbDmDocDate.Checked),RegPath);&#13;&#10;    Реестр_Записать('cbEmpMode',BoolToStr(cbEmpMode.Checked),RegPath);&#13;&#10;    Реестр_Записать('rbDocTran',BoolToStr(rbDocTran.Checked),RegPath);&#13;&#10;    Реестр_Записать('chbDatePrint', BoolToStr(chbDatePrint.ChEcked), RegPath);&#13;&#10;&#13;&#10;    If rbDocRec.Checked Then&#13;&#10;    Begin&#13;&#10;      ReportSummary1.Child := ChRec;&#13;&#10;      ReportTitle1.Child := ChTopRec;&#13;&#10;    End&#13;&#10;    Else&#13;&#10;    Begin&#13;&#10;      ReportSummary1.Child := ChTran;&#13;&#10;      ReportTitle1.Child := ChTopTran;&#13;&#10;    End;&#13;&#10;&#13;&#10;    dq_Doc.SQL.Text := 'SELECT TOP 1 m.DocID, ' +&#13;&#10;                       IIF(chbDatePrint.Checked, 'NULL',&#13;&#10;                       IIF(rbDmDocDate.Checked, 'm.DocDate', 'CAST(''' + FormatDateTime('yyyymmdd', deDocDate.Date) + ''' AS smalldatetime)')) + ' DocDate,'#13 +&#13;&#10;                       'ro.Note1, s1.StockName StockName, s1.Notes AS StockNotes, s3.StockName CompStockName, s3.Notes CompStockNotes,'+&#13;&#10;                       's2.StockName AS NewStockName, s2.Notes AS NewStockNotes, m.Notes, LTRIM(RTRIM(REPLACE(c.Contract1,''  '', '' ''))) Contract1, ' +&#13;&#10;                       'ro.Note2, ro.PostIndex, ro.City, ro.Address, roc.AccountCC, rb.BankID, rb.BankName,'#13 +&#13;&#10;                       'ro.Code, ro.TaxCode, ro.TaxRegNo,'#13 +&#13;&#10;                       IIF(chbExc.Checked, IIF(rbDocRec.Checked, 's1.Notes', 's2.Notes'), 'm.Note1') + ' AS DocNote1, ' +&#13;&#10;                       'dbo.af_GetContrData(z.ParentChID, z.ParentDocCode,''ContrID3'') ContrID,'#13 +&#13;&#10;                       'dbo.af_GetContrData(z.ParentChID, z.ParentDocCode,''BDate'') BDate,'#13 +&#13;&#10;                       'CAST((SELECT COUNT(*) FROM dbo.at_z_ContractsAdd zca WITH(NOLOCK) WHERE zca.HaveSeal = 1 AND zca.ChID = (SELECT TOP 1 ChID FROM dbo.at_z_Contracts WITH(NOLOCK) WHERE CompID = m.CompID AND OurID = ' + dq_Ours.Fields[0].AsString + #13 +         &#13;&#10;                       'AND Status = 1 ORDER BY EDate DESC, ChID ASC)) + (SELECT COUNT(*) FROM dbo.at_z_ContractsAdd zca WITH(NOLOCK) WHERE zca.HaveStamp = 1 '#13 +&#13;&#10;                       'AND zca.ChID = (SELECT TOP 1 ChID FROM dbo.at_z_Contracts WITH(NOLOCK) WHERE CompID = m.CompID AND OurID = ' + dq_Ours.Fields[0].AsString + ' AND Status = 1 ORDER BY EDate DESC, ChID ASC)) AS BIT) HaveStamp'#13 +        &#13;&#10;                       'FROM ' + IIF(chbExc.Checked, 'av_t_Exc m', 'av_t_Acc m') + #13 +&#13;&#10;                       'JOIN r_Ours ro WITH(NOLOCK) ON ro.OurID=m.OurID'#13 +&#13;&#10;                       'JOIN r_Comps c WITH(NOLOCK) ON c.CompID=m.CompID'#13 +&#13;&#10;                       'JOIN r_Stocks s1 WITH(NOLOCK) ON s1.StockID=m.StockID'#13 +&#13;&#10;                       'JOIN r_Stocks s2 WITH(NOLOCK) ON s2.StockID=m.' +IIF(chbExc.Checked, 'NewStockID', 'StockID') + #13 +&#13;&#10;                       'LEFT JOIN r_Stocks s3 WITH(NOLOCK) ON s3.CompID=m.CompID'#13 +&#13;&#10;                       'JOIN r_OursCC roc WITH(NOLOCK) ON roc.OurID = m.OurID AND roc.DefaultAccount = 1'#13 +&#13;&#10;                       'JOIN r_Banks rb WITH(NOLOCK) ON rb.BankID = roc.BankID'#13 +&#13;&#10;                       'LEFT JOIN z_DocLinks z WITH(NOLOCK) ON z.ChildDocCode = ' + IIF(chbExc.Checked, '11021', '11001') + ' AND z.ParentDocCode = 666028 AND z.ChildChID = m.ChID'#13 +&#13;&#10;                       'WHERE (' + СписокВУсловиеSQL('m.DocID',edtDocID.Text) + ') AND m.OurID = ' + dq_Ours.Fields[0].AsString + #13 +&#13;&#10;                       'ORDER BY 1';&#13;&#10;    dq_Doc.Open;&#13;&#10;    If dq_Doc.IsEmpty Then&#13;&#10;      Begin&#13;&#10;        MessageDlg('Нет данных для печати!!! Проверьте наличие необходимых данных, заполнение справочника &#34;Внутренних фирм: Расчётные счета&#34;.', mtError, mbOk,0);&#13;&#10;        Engine.StopReport;&#13;&#10;        Exit;&#13;&#10;      End;&#13;&#10;&#13;&#10;    dq_DocD.SQL.Text := 'SELECT m.ProdID, p.Notes AS ProdName, p.Article1, m.UM, '#13 +&#13;&#10;                        // Описание товара для акта на передачу (Select)&#13;&#10;                        IIF(rbDocTran.Checked, 'ISNULL(pv.VarName + ''. '','''') + ISNULL(pv.VarValue,'''') ','NULL') + ' VarValue,'#13 +&#13;&#10;                        // Режим цены&#13;&#10;                        IIF(rbPmDoc.Checked, 'm.PriceCC_wt', 'mp.PriceMC*' +&#13;&#10;                        // Режим курса&#13;&#10;                        IIF(rbKmDocKurs.Checked, 'dbo.af_GetRateCCByDate(mp.CurrID,a.DocDate)', FloatToSQL(StrToFloat(edtKurs.Text)))) + ' AS PriceCC,'#13 +&#13;&#10;                        'SUM(m.Qty) AS Qty, ' +&#13;&#10;                        // Общее количество&#13;&#10;                        '(SELECT SUM(Qty) FROM ' + IIF(chbExc.Checked, 'av_t_ExcD', 'av_t_AccD') + ' WHERE ChID = m.ChID) TQty,'#13 +&#13;&#10;                        // Общая сумма&#13;&#10;                        '(SELECT ' + IIF(rbPmDoc.Checked, 'SUM(d.SumCC_wt)', 'SUM(d.Qty*rp.PriceMC)' +&#13;&#10;                        // Режим курса&#13;&#10;                        IIF(rbKmDocKurs.Checked And rbKmDocKurs.Enabled, '*dbo.af_GetRateCCByDate(rp.CurrID,a.DocDate)', '*' + FloatToSQL(StrToFloat(edtKurs.Text)))) +&#13;&#10;                        ' FROM ' + IIF(chbExc.Checked, 'av_t_ExcD', 'av_t_AccD') + ' d LEFT JOIN r_ProdMP rp WITH(NOLOCK) ON rp.ProdID=d.ProdID AND rp.PLID = ' + VarToStr(cbPLs.KeyValue) +&#13;&#10;                        ' WHERE d.ChID = m.ChID' + IIF(rbKmDocKurs.Checked And rbKmDocKurs.Enabled, ' GROUP BY rp.CurrID', '') + ') TSumCC'#13 +&#13;&#10;                        'FROM ' + IIF(chbExc.Checked, 'av_t_ExcD', 'av_t_AccD') + ' m'#13 +&#13;&#10;                        'JOIN ' + IIF(chbExc.Checked, 'av_t_Exc', 'av_t_Acc') + ' a ON a.ChID=m.ChID'#13 +&#13;&#10;                        'JOIN r_Prods p WITH(NOLOCK) ON p.ProdID=m.ProdID'#13 +&#13;&#10;                        'LEFT JOIN r_ProdMP mp WITH(NOLOCK) ON mp.ProdID=m.ProdID AND mp.PLID = ' + VarToStr(cbPLs.KeyValue) + #13 +&#13;&#10;                         // Описание товара для акта на передачу (Table)&#13;&#10;                        IIF(rbDocTran.Checked, 'LEFT JOIN r_ProdValues pv WITH(NOLOCK) ON pv.ProdID=m.ProdID'#13,'') +&#13;&#10;                        'WHERE (' + СписокВУсловиеSQL('a.DocID',edtDocID.Text) + ') AND a.OurID = ' + dq_Ours.Fields[0].AsString + #13 +&#13;&#10;                        'GROUP BY m.ChID,m.ProdID, p.Notes, p.Article1, m.UM, ' +&#13;&#10;                        // Описание товара для акта на передачу (Grouping)&#13;&#10;                        IIF(rbDocTran.Checked,'ISNULL(pv.VarName + ''. '','''') + ISNULL(pv.VarValue,''''),'#13,'') +&#13;&#10;                        IIF(rbPmDoc.Checked, 'm.PriceCC_wt', 'mp.PriceMC*' +&#13;&#10;                        IIF(rbKmDocKurs.Checked, 'dbo.af_GetRateCCByDate(mp.CurrID,a.DocDate)', FloatToSQL(StrToFloat(edtKurs.Text)))) + #13 +&#13;&#10;                        IIF(rbKmDocKurs.Checked, ',a.DocDate', '') + #13 +&#13;&#10;                        'ORDER BY MAX(m.SrcPosID)';&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure rbDocRecOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  chbAcc.Enabled := rbDocRec.Checked;&#13;&#10;  chbExc.Enabled := rbDocRec.Checked;&#13;&#10;  chbAcc.Checked := rbDocRec.Checked;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure rbDocTranOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  chbAcc.Enabled := Not rbDocTran.Checked;&#13;&#10;  chbExc.Enabled := Not rbDocTran.Checked;&#13;&#10;  chbExc.Checked := rbDocTran.Checked;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure chbAccOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  RefreshEmps(Sender);&#13;&#10;  chbExc.Checked := Not chbAcc.Checked;&#13;&#10;  chbDatePrint.Enabled :=  chbAcc.Checked;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure chbExcOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  RefreshEmps(Sender);&#13;&#10;  chbAcc.Checked := Not chbExc.Checked;&#13;&#10;  chbDatePrint.Enabled := Not chbExc.Checked;&#13;&#10;  chbDatePrint.Checked := Not chbExc.Checked And Not chbDatePrint.Enabled;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." StoreInDFM="False" PropData="">
