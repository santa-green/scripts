    <TfrxBDEQuery Name="dq_Doc" UserName="dq_Doc" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="True" SQL.Text="DECLARE @RetChID INT = CASE :DocCode&#13;&#10; WHEN 11003 THEN :DocChID&#13;&#10; WHEN 11012 THEN (SELECT MAX(d.ChID) FROM t_Inv m WITH(NOLOCK)&#13;&#10;    JOIN t_Ret d WITH(NOLOCK) ON m.TaxDocID = d.TaxDocID AND m.DocDate = d.DocDate AND d.OurID = m.OurID AND d.StockID = m.StockID AND d.CodeID2 = m.CodeID2 AND d.CodeID2 IN (43,44)&#13;&#10;    WHERE m.ChID = :DocChID)&#13;&#10;END&#13;&#10;&#13;&#10;DECLARE @InvChID INT = CASE :DocCode&#13;&#10; WHEN 11012 THEN :DocChID&#13;&#10; WHEN 11003 THEN (SELECT MAX(m.ChID) FROM t_Inv m WITH(NOLOCK)&#13;&#10;    JOIN t_Ret d WITH(NOLOCK) ON m.TaxDocID = d.TaxDocID AND m.DocDate = d.DocDate AND d.OurID = m.OurID AND d.StockID = m.StockID AND d.CodeID2 = m.CodeID2 AND d.CodeID2 IN (43,44)&#13;&#10;    WHERE d.ChID = :DocChID)&#13;&#10;END&#13;&#10;DECLARE @LEN TINYINT = 7&#13;&#10;IF EXISTS(SELECT * FROM dbo.t_Ret WITH(NOLOCK) WHERE ChID = @RetChID AND SrcTaxDocDate &#60; '20110301')&#13;&#10;  SET @LEN = 10&#13;&#10;&#13;&#10;SELECT TOP 1&#13;&#10;@RetChID RetChID, @InvChID InvChID, @LEN Length, ior.OrderID,  &#13;&#10;CASE WHEN im.SrcDocID IS NOT NULL AND (im.SrcDocID LIKE '[0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]' OR im.SrcDocID LIKE '[0-9][0-9] [0-9][0-9][0-9] [0-9][0-9][0-9]') AND im.SrcDocID != ior.OrderID THEN im.SrcDocID END [PAYMENTORDERNUMBER],    &#13;&#10;CASE WHEN rtm.TaxDocDate &#60; '20140301' THEN 1 ELSE 2 END TaxPageForm,    &#13;&#10;dbo.af_GetGapedNumber(LEFT(rtm.SrcTaxDocID,@LEN), @LEN) GSrcTaxDocID, rtm.SrcTaxDocID SrcTaxDocID,&#13;&#10;dbo.af_GetGapedNumber(dbo.zf_DateToStr(rtm.SrcTaxDocDate), DEFAULT) GSrcTaxDocDate, dbo.zf_DateToStr(rtm.SrcTaxDocDate) SrcTaxDocDate,&#13;&#10;dbo.af_GetGapedNumber(LEFT(rtm.TaxDocID,8), 8) GTaxDocID, LEFT(rtm.TaxDocID,8) TaxDocID,&#13;&#10;dbo.af_GetGapedNumber(dbo.zf_DateToStr(rtm.TaxDocDate), DEFAULT) GTaxDocDate, rtm.TaxDocDate,&#13;&#10;dbo.af_GetGapedNumber(o.Phone, 10) AS O_GPhone,&#13;&#10;rtm.CodeID2, o.Note2 OurName, o.Region AS O_Region,&#13;&#10;o.City AS O_City, o.PostIndex AS O_PostIndex,&#13;&#10;o.[Address] AS O_Address, o.Code AS O_Code,&#13;&#10;dbo.af_GetGapedNumber(o.TaxCode, 12) AS O_GTaxCode, o.TaxCode AS O_TaxCode,&#13;&#10;o.TaxRegNo AS O_TaxRegNo, o.Phone AS O_Phone, o.Note2,&#13;&#10;c.CompName, c.City AS C_City, dbo.af_GetGapedNumber(c.Phone1, 10) AS C_GPhone, c.Phone1 AS C_Phone, c.TaxPayer,&#13;&#10;dbo.af_GetGapedNumber(c.TaxCode, 12) AS C_GTaxCode, c.TaxCode AS C_TaxCode,&#13;&#10;c.TaxRegNo AS C_TaxRegNo,&#13;&#10;c.TaxCompName AS TaxCompName, c.TaxAddress AS TaxAddress, c.TaxPhone AS TaxPhone,&#13;&#10;CASE WHEN c.Contract2 IS NOT NULL AND c.Contract2 &#60;&#62; '' AND c.CanInvoicing = 1 THEN c.License1 ELSE c.Code END AS C_Code,&#13;&#10;CASE WHEN c.IsBranch = 1 AND c.CanInvoicing = 1 THEN CHAR(13) +  REPLACE(c.Contract3, ' ,', ',') END AS Contract3,&#13;&#10;CASE WHEN c.IsBranch = 1 AND c.CanInvoicing = 1 THEN c.Contract2 ELSE c.Contract1 END Contract1,&#13;&#10;CASE WHEN c.IsBranch = 1 AND c.CanInvoicing = 1 THEN LTRIM(RTRIM(REPLACE(c.Contract3, ' ,', ','))) ELSE c.Address + ', ' + c.City + ISNULL(', ' + c.Region, '') + ISNULL(', ' + CASE WHEN LEN(c.PostIndex) &#62; 4 THEN c.PostIndex END,'') END AS C_Address,&#13;&#10;REPLACE(REPLACE(dbo.af_GetFiltered(REPLACE(REPLACE(zc.ContrID,'-','@'),'/','#')),'@','-'),'#','/') ContrID,&#13;&#10;ru.RefName ContrType,&#13;&#10;dbo.af_GetGapedNumber(dbo.zf_DateToStr(zc.BDate), DEFAULT) GBDate,&#13;&#10;zc.BDate,&#13;&#10;CorReason = '',&#13;&#10;CASE WHEN rtm.TaxDocDate !&#60; '20120101' AND rtm.StateCode != 193 AND c.TaxPayer = 1 THEN 'X' END AS IsERPN,&#13;&#10;CASE c.TaxPayer WHEN 1 THEN '' ELSE 'X' END IsHeld,&#13;&#10;CASE c.TaxPayer WHEN 1 THEN '' ELSE '0  2' END ReasonCode,z2.ParentDocid, z2.ParentDocDate              &#13;&#10;FROM dbo.t_Ret rtm WITH(NOLOCK)&#13;&#10;LEFT JOIN dbo.t_Inv im ON im.ChID = @InvChID&#13;&#10;JOIN dbo.r_Ours o WITH(NOLOCK) ON o.OurID = rtm.OurID&#13;&#10;OUTER APPLY dbo.af_GetCompReqs(rtm.CompID, rtm.TaxDocDate) c&#13;&#10;LEFT JOIN dbo.z_DocLinks z WITH(NOLOCK) ON z.ChildDocCode = 11003 AND z.ParentDocCode = 666028 AND z.ChildChID = rtm.ChID&#13;&#10;LEFT JOIN dbo.z_DocLinks z2 WITH(NOLOCK) ON z2.ChildDocCode = 11003 AND z2.ParentDocCode = 11012 AND z2.ChildChID = rtm.ChID    &#13;&#10;LEFT JOIN dbo.at_z_Contracts zc WITH(NOLOCK) ON zc.ChID = z.ParentChID&#13;&#10;LEFT JOIN dbo.r_Uni ru WITH(NOLOCK) ON ru.RefTypeID = 6660120 AND ru.RefID = zc.OffTypeID&#13;&#10;LEFT JOIN dbo.z_DocLinks zdl0 WITH(NOLOCK) ON zdl0.ChildChID = im.ChID AND zdl0.ChildDocCode = 11012 AND zdl0.ParentDocCode = 666004&#13;&#10;LEFT JOIN dbo.z_DocLinks zdl1 WITH(NOLOCK) ON zdl1.ChildChID = zdl0.ParentChID AND zdl1.ParentDocCode = 11221 AND zdl1.ChildDocCode = zdl0.ParentDocCode&#13;&#10;LEFT JOIN dbo.t_IORec ior WITH(NOLOCK) ON ior.ChID = zdl1.ParentChID    &#13;&#10;WHERE rtm.TaxDocID &#62; 0&#13;&#10;AND rtm.ChID = @RetChID&#13;&#10;OPTION(FAST 1)" DatabaseName="dbMain" SessionName="Default" PropData=""/>
    <TfrxBDEQuery Name="dq_DocD" UserName="dq_DocD" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="dq_Doc" IgnoreDupParams="True" SQL.Text="SELECT TaxDocDate, CorReason, ProdID, ExtProdID, UAProdName, CstProdCode, UM, SUM(RetQty) RetQty, SUM(InvQty) InvQty, SUM([TAB1_A5]) / PqQty [TAB1_A5], [TAB1_A6] * PqQty [TAB1_A6],&#13;&#10;InvPriceCC_nt, RetPriceCC_nt, [TAB1_A7] * PqQty [TAB1_A7], SUM([TAB1_A8]) / PqQty [TAB1_A8], SUM([TAB1_A9]) [TAB1_A9], CustInfo&#13;&#10;, PqQty      &#13;&#10;FROM&#13;&#10;(&#13;&#10;SELECT&#13;&#10;  rtm.TaxDocDate,&#13;&#10;  CASE WHEN rtm.CodeID2 IN (19,69,56) THEN 'Повернення'&#13;&#10;    WHEN rtm.CodeID2 IN (43) THEN 'Коригування за ціною'&#13;&#10;    WHEN rtm.CodeID2 IN (44,58) THEN 'Коригування за кількістю'&#13;&#10;    ELSE ''&#13;&#10;  END CorReason,&#13;&#10;  rtd.ProdID,&#13;&#10;  ec.ExtProdID, LTRIM(RTRIM(ISNULL(NULLIF(ec.ExtProdName,''), rp.Notes))) UAProdName,&#13;&#10;  pp.CstProdCode,&#13;&#10;  rp.UM,&#13;&#10;  (SELECT SUM(Qty) FROM t_RetD WITH(NOLOCK) WHERE ChID = rtm.ChID AND ProdID = rtd.ProdID AND PPID = rtd.PPID) RetQty,&#13;&#10;  (SELECT SUM(Qty) FROM t_InvD WITH(NOLOCK) WHERE ChID = im.ChID AND ProdID = rtd.ProdID AND PPID = rtd.PPID) InvQty,&#13;&#10;  CAST(CASE rtm.CodeID2 WHEN 43 THEN 0 ELSE SUM(ISNULL(id.Qty, 0)) - SUM(rtd.Qty) END AS NUMERIC(21,6)) [TAB1_A5],&#13;&#10;  CAST(CASE rtm.CodeID2 WHEN 43 THEN 0 ELSE rtd.PriceCC_nt END AS NUMERIC(21,12)) [TAB1_A6],&#13;&#10;  MIN(ISNULL(id.PriceCC_nt, 0)) InvPriceCC_nt,&#13;&#10;  MIN(rtd.PriceCC_nt) RetPriceCC_nt,&#13;&#10;  CAST(CASE rtm.CodeID2 WHEN 43 THEN ISNULL(id.PriceCC_nt, 0) - rtd.PriceCC_nt ELSE 0 END AS NUMERIC(21,12)) [TAB1_A7],&#13;&#10;  CAST(CASE rtm.CodeID2 WHEN 43 THEN SUM(rtd.Qty) ELSE 0 END AS NUMERIC(21,6)) [TAB1_A8],&#13;&#10;  CAST(SUM(ISNULL(id.Qty, 0)*ISNULL(id.PriceCC_nt, 0)) - SUM(rtd.Qty*rtd.PriceCC_nt) AS NUMERIC(21,2)) [TAB1_A9],&#13;&#10;  'ВМД № ' + pp.CstDocCode + ' від '+ dbo.zf_DateToStr(pp.CstDocDate) CustInfo&#13;&#10;  , ISNULL(pq.Qty, 1) as PqQty      &#13;&#10;FROM dbo.t_Ret rtm&#13;&#10;JOIN (SELECT ChID, ProdID, PPID, PriceCC_nt, SUM(Qty) Qty FROM dbo.t_RetD WITH(NOLOCK) WHERE ChID = :RetChID GROUP BY ChID, ProdID, PPID, PriceCC_nt) rtd ON rtd.ChID = rtm.ChID&#13;&#10;LEFT JOIN dbo.t_Inv im ON im.ChID = :InvChID&#13;&#10;LEFT JOIN (SELECT ChID, ProdID, PPID, PriceCC_nt, SUM(Qty) Qty FROM dbo.t_InvD WITH(NOLOCK) WHERE ChID = :InvChID GROUP BY ChID, ProdID, PPID, PriceCC_nt) id ON id.ChID = im.ChID AND id.ProdID = rtd.ProdID AND id.PPID = rtd.PPID&#13;&#10;JOIN dbo.r_Prods rp WITH(NOLOCK) ON rp.ProdID = rtd.ProdID&#13;&#10;JOIN dbo.t_PInP pp WITH(NOLOCK) ON pp.ProdID = rtd.ProdID AND pp.PPID = rtd.PPID&#13;&#10;LEFT JOIN dbo.r_ProdEC ec WITH(NOLOCK) ON ec.CompID = rtm.CompID AND ec.ProdID = rp.ProdID&#13;&#10;LEFT JOIN r_prodmq pq WITH(NOLOCK) ON rp.ProdID=pq.ProdID AND pq.UM LIKE '%метро/един%'    &#13;&#10;WHERE rtm.ChID = :RetChID&#13;&#10;GROUP BY rtm.ChID, im.ChID, rtd.PPID,&#13;&#10;rtm.TaxDocDate, ec.ExtProdID, LTRIM(RTRIM(ISNULL(NULLIF(ec.ExtProdName,''), rp.Notes))), rtd.ProdID, pp.CstProdCode, rp.UM, rtm.CodeID2,&#13;&#10;CAST(CASE rtm.CodeID2 WHEN 43 THEN 0 ELSE rtd.PriceCC_nt END AS NUMERIC(21,12)),&#13;&#10;CAST(CASE rtm.CodeID2 WHEN 43 THEN ISNULL(id.PriceCC_nt, 0) - rtd.PriceCC_nt ELSE 0 END AS NUMERIC(21,12)),&#13;&#10;'ВМД № ' + pp.CstDocCode + ' від '+ dbo.zf_DateToStr(pp.CstDocDate)&#13;&#10;, pq.Qty         &#13;&#10;) data&#13;&#10;GROUP BY TaxDocDate, CorReason, ProdID, ExtProdID, UAProdName, CstProdCode, UM, [TAB1_A6],&#13;&#10;InvPriceCC_nt, RetPriceCC_nt, [TAB1_A7], CustInfo&#13;&#10;, PqQty&#13;&#10;HAVING SUM([TAB1_A7]) != 0 OR SUM([TAB1_A5]) != 0                                                &#13;&#10;ORDER BY ProdID  " DatabaseName="dbMain" SessionName="Default" PropData=""/>
