<TfrxReport Version="4.12" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.Author="sev12" ReportOptions.CreateDate="39762,7062155093" ReportOptions.Description.Text="" ReportOptions.LastChange="43727,6673965278" ScriptLanguage="PascalScript" ScriptText.Text="&#13;&#10;begin&#13;&#10;  dq_Doc.Open;    &#13;&#10;  dq_DocD.Open;&#13;&#10;    &#13;&#10;  If dq_Doc.IsEmpty or dq_DocD.IsEmpty Then&#13;&#10;  Begin&#13;&#10;    MessageDlg('Данные о корректировках отсутствуют! Проверьте связь между Расходной и Возвратной накладными по следующим полям:'#13 +&#13;&#10;      '&#34;Номер нал. источника&#34;, &#34;Дата&#34;, &#34;Фирма&#34;, &#34;Признак 2&#34;.', mtError, mbOk, 0);&#13;&#10;    Engine.StopReport;&#13;&#10;  End;&#13;&#10;      &#13;&#10;&#13;&#10;  //Разделение на корректировки по цене/количеству&#13;&#10;  if dq_Doc.FieldByName('CodeID2').Value = 43 Then&#13;&#10;  Begin&#13;&#10;    AddMemo1072.Text := '[dq_DocD.&#34;TAB1_A8&#34;]';&#13;&#10;    AddMemo1021.Text := 'Кількість'#13#10'МО'#13#10'(-\+)';        &#13;&#10;    AddMemo1073.Text := '[dq_DocD.&#34;TAB1_A7&#34;]';&#13;&#10;    AddMemo1022.Text := 'Коригування'#13#10'цiни'#13#10'(-\+)';          &#13;&#10;  End;  &#13;&#10;end." StoreInDFM="False" PropData="">
