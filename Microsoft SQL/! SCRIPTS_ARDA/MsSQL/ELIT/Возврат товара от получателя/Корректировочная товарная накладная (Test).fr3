    <TfrxBDEQuery Name="dq_Doc" UserName="dq_Doc" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="True" SQL.Text="DECLARE @RetChID INT = CASE :DocCode&#13;&#10; WHEN 11003 THEN :DocChID&#13;&#10; WHEN 11012 THEN (SELECT MAX(d.ChID) FROM t_Inv m WITH(NOLOCK)&#13;&#10;    JOIN t_Ret d WITH(NOLOCK) ON m.TaxDocID = d.TaxDocID AND m.CompID = d.CompID AND m.DocDate = d.DocDate AND d.OurID = m.OurID AND d.StockID = m.StockID AND d.CodeID2 = m.CodeID2 AND d.CodeID2 IN (43,44,38)&#13;&#10;    WHERE m.ChID = :DocChID)&#13;&#10;END&#13;&#10;&#13;&#10;DECLARE @InvChID INT = CASE :DocCode&#13;&#10; WHEN 11012 THEN :DocChID&#13;&#10; WHEN 11003 THEN (SELECT MAX(m.ChID) FROM t_Inv m WITH(NOLOCK)&#13;&#10;    JOIN t_Ret d WITH(NOLOCK) ON m.TaxDocID = d.TaxDocID AND m.CompID = d.CompID AND m.DocDate = d.DocDate AND d.OurID = m.OurID AND d.StockID = m.StockID AND d.CodeID2 = m.CodeID2 AND d.CodeID2 IN (43,44,38)&#13;&#10;    WHERE d.ChID = :DocChID)&#13;&#10;END  &#13;&#10;&#13;&#10;SELECT TOP 1&#13;&#10;@RetChID RetChID, @InvChID InvChID,  &#13;&#10;rtm.SrcTaxDocID, rtm.SrcTaxDocDate, LEFT(rtm.TaxDocID,8) TaxDocID,&#13;&#10;rtm.TaxDocDate, rtm.CodeID2, o.OurName, o.Region O_Region,&#13;&#10;o.City AS O_City, o.PostIndex AS O_PostIndex,&#13;&#10;o.Address AS O_Address, o.Code AS O_Code,&#13;&#10;o.TaxCode AS O_TaxCode, o.TaxRegNo AS O_TaxRegNo, o.Phone AS O_Phone, o.Note2,&#13;&#10;c.CompName, c.City AS C_City, c.Phone1 AS C_Phone,&#13;&#10;c.TaxCode AS C_TaxCode, c.TaxRegNo AS C_TaxRegNo, c.TaxPayer,&#13;&#10;c.Contract1, &#13;&#10;c.Address + ', ' + c.City + ISNULL(', ' + c.Region, '') + ISNULL(', ' + CASE WHEN LEN(c.PostIndex) &#62; 4 THEN c.PostIndex END,'') AS C_Address,&#13;&#10;c.Code AS C_Code,&#13;&#10;zcc.ContrID, ru.RefName ContrType, zcc.BDate,&#13;&#10;res.MilCalcGrp, ShortUAEmpName = res.UAEmpLastName + ' ' + LEFT(res.UAEmpFirstName, 1) + '.' + LEFT(res.UAEmpParName,1) + '.',&#13;&#10;zca.HaveSeal, zca.HaveStamp                                                         &#13;&#10;FROM dbo.t_Ret rtm WITH(NOLOCK)&#13;&#10;LEFT JOIN dbo.t_Inv im ON im.ChID = @InvChID    &#13;&#10;JOIN dbo.r_Ours o WITH(NOLOCK) ON o.OurID = rtm.OurID&#13;&#10;OUTER APPLY [dbo].[af_GetCompReqs](rtm.CompID, rtm.SrcTaxDocDate) c&#13;&#10;LEFT JOIN dbo.r_CompsAdd ca WITH(NOLOCK) ON ca.CompID = rtm.CompID AND ca.CompAdd = rtm.Address&#13;&#10;LEFT JOIN dbo.z_DocLinks z WITH(NOLOCK) ON z.ChildDocCode = 11003 AND z.ParentDocCode = 666028 AND z.ChildChID = rtm.ChID&#13;&#10;LEFT JOIN dbo.at_z_Contracts zcc WITH(NOLOCK) ON zcc.ChID = z.ParentChID&#13;&#10;LEFT JOIN dbo.r_Uni ru WITH(NOLOCK) ON ru.RefTypeID = 6660120 AND ru.RefID = zcc.OffTypeID&#13;&#10;LEFT JOIN dbo.at_z_ContractsAdd zca WITH(NOLOCK) ON zca.ChID = zcc.ChID AND zca.CompAddID = ca.CompAddID&#13;&#10;JOIN dbo.r_Stocks rs WITH(NOLOCK) ON rs.StockID=rtm.StockID&#13;&#10;JOIN dbo.at_r_OurAccSector ros WITH(NOLOCK) ON ros.OurID = rtm.OurID AND ros.StockGID = rs.StockGID&#13;&#10;JOIN dbo.r_Emps res WITH(NOLOCK) ON res.EmpID = ros.EmpID    &#13;&#10;WHERE rtm.ChID = @RetChID&#13;&#10;OPTION(FAST 1)&#13;&#10;  " DatabaseName="dbMain" SessionName="Default" PropData=""/>
    <TfrxBDEQuery Name="dq_DocD" UserName="dq_DocD" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="dq_Doc" IgnoreDupParams="True" SQL.Text="SELECT TaxDocDate, CorReason, ProdID, UAProdName, CstProdCode, UM, SUM(RetQty) RetQty, SUM(InvQty) InvQty, SUM(InvSumCC_wt) InvSumCC_wt,  &#13;&#10;SUM(RetSumCC_wt) RetSumCC_wt, SUM([TAB1_A5]) [TAB1_A5], [TAB1_A6],&#13;&#10;InvPriceCC_nt, RetPriceCC_nt, [TAB1_A7], SUM([TAB1_A8]) [TAB1_A8], SUM([TAB1_A9]) [TAB1_A9], SUM([A1_9]) [A1_9]&#13;&#10;FROM&#13;&#10;(&#13;&#10;SELECT&#13;&#10;  rtm.TaxDocDate,&#13;&#10;  CASE WHEN rtm.CodeID2 IN (19,69,56) THEN 'Повернення'&#13;&#10;    WHEN rtm.CodeID2 IN (43,38) THEN 'Коригування за ціною'&#13;&#10;    WHEN rtm.CodeID2 IN (44) THEN 'Коригування за кількістю'&#13;&#10;    ELSE ''&#13;&#10;  END CorReason,&#13;&#10;  rtd.ProdID,&#13;&#10;  (case when  im.CodeID1 = 63  and im.CodeID2 IN (18,43, 44, 69) and im.CodeID3 = 4   &#13;&#10;and im.taxdocdate &#62;= '20161201'and rp.Article2 like '%&#38;M%' and im.CompID Not between 7000 and 7999 and  im.CompID Not between 10790 and 10799 and r.CodeID4 != 5002  then  rp.Article2&#13;&#10;else ISNULL(NULLIF(ec.ExtProdName,''), rp.Notes) end)  UAProdName,&#13;&#10;  pp.CstProdCode,&#13;&#10;  rp.UM,&#13;&#10;  (SELECT SUM(Qty) FROM t_RetD WITH(NOLOCK) WHERE ChID = :RetChID AND ProdID = rtd.ProdID AND PPID = rtd.PPID) RetQty,&#13;&#10;  (SELECT SUM(Qty) FROM t_InvD WITH(NOLOCK) WHERE ChID = :InvChID AND ProdID = rtd.ProdID AND PPID = rtd.PPID) InvQty,&#13;&#10;  CAST(CASE WHEN rtm.CodeID2 IN (43,38) THEN 0 ELSE SUM(ISNULL(id.Qty, 0)) - SUM(rtd.Qty) END AS NUMERIC(21,6)) [TAB1_A5],&#13;&#10;  CAST(CASE WHEN rtm.CodeID2 IN (43,38) THEN 0 ELSE rtd.PriceCC_nt END AS NUMERIC(21,12)) [TAB1_A6],&#13;&#10;  MIN(ISNULL(id.PriceCC_nt, 0)) InvPriceCC_nt,&#13;&#10;  SUM(id.SumCC_wt) InvSumCC_wt,                   &#13;&#10;  MIN(rtd.PriceCC_nt) RetPriceCC_nt,&#13;&#10;  SUM(rtd.SumCC_wt) RetSumCC_wt,      &#13;&#10;  CAST(CASE WHEN rtm.CodeID2 IN (43,38) THEN ISNULL(id.PriceCC_nt, 0) - rtd.PriceCC_nt ELSE 0 END AS NUMERIC(21,12)) [TAB1_A7],&#13;&#10;  CAST(CASE WHEN rtm.CodeID2 IN (43,38) THEN SUM(rtd.Qty) ELSE 0 END AS NUMERIC(21,6)) [TAB1_A8],&#13;&#10;  CAST(SUM(ISNULL(id.Qty, 0)*ISNULL(id.PriceCC_nt, 0)) - SUM(rtd.Qty*rtd.PriceCC_nt) AS NUMERIC(21,2)) [TAB1_A9],&#13;&#10;  CAST(SUM(ISNULL(id.Qty, 0)*ISNULL(id.Tax, 0)) - SUM(rtd.Qty*rtd.Tax) AS NUMERIC(21,2)) [A1_9]&#13;&#10;FROM dbo.t_Ret rtm&#13;&#10;JOIN (SELECT ChID, ProdID, PPID, PriceCC_nt, Tax, SUM(SumCC_wt) SumCC_wt, SUM(Qty) Qty FROM dbo.t_RetD WITH(NOLOCK) WHERE ChID = :RetChID GROUP BY ChID, ProdID, PPID, PriceCC_nt, Tax) rtd ON rtd.ChID = rtm.ChID&#13;&#10;LEFT JOIN dbo.t_Inv im ON im.ChID = :InvChID&#13;&#10;LEFT JOIN (SELECT ChID, ProdID, PPID, PriceCC_nt, Tax, SUM(SumCC_wt) SumCC_wt, SUM(Qty) Qty FROM dbo.t_InvD WITH(NOLOCK) WHERE ChID = :InvChID GROUP BY ChID, ProdID, PPID, PriceCC_nt, Tax) id ON id.ChID = im.ChID AND id.ProdID = rtd.ProdID AND id.PPID = rtd.PPID&#13;&#10;JOIN dbo.r_Prods rp WITH(NOLOCK) ON rp.ProdID = rtd.ProdID&#13;&#10;JOIN dbo.t_PInP pp WITH(NOLOCK) ON pp.ProdID = rtd.ProdID AND pp.PPID = rtd.PPID&#13;&#10;LEFT JOIN dbo.r_ProdEC ec WITH(NOLOCK) ON ec.ProdID = rtm.CompID AND ec.ProdID = rp.ProdID&#13;&#10;JOIN r_Comps r ON im.CompID =r.CompID &#13;&#10;WHERE rtm.ChID = :RetChID&#13;&#10;GROUP BY rtm.ChID, im.ChID, rtd.PPID,&#13;&#10;rtm.TaxDocDate, LTRIM(RTRIM(ISNULL(NULLIF(ec.ExtProdName,''), rp.Notes))), rtd.ProdID,&#13;&#10;pp.CstProdCode, rp.UM, rtm.CodeID2,&#13;&#10;im.CodeID1 , im.CodeID2 , im.CodeID3 ,r.CodeID4 ,rp.Article2, im.taxdocdate,   im.CompID, ec.ExtProdName, rp.Notes,                      &#13;&#10;CAST(CASE WHEN rtm.CodeID2 IN (43,38) THEN 0 ELSE rtd.PriceCC_nt END AS NUMERIC(21,12)),&#13;&#10;CAST(CASE WHEN rtm.CodeID2 IN (43,38) THEN ISNULL(id.PriceCC_nt, 0) - rtd.PriceCC_nt ELSE 0 END AS NUMERIC(21,12))&#13;&#10;) data&#13;&#10;GROUP BY TaxDocDate, CorReason, ProdID, UAProdName, CstProdCode, UM, [TAB1_A6],&#13;&#10;InvPriceCC_nt, RetPriceCC_nt, [TAB1_A7]&#13;&#10;ORDER BY ProdID&#13;&#10;      " DatabaseName="dbMain" SessionName="Default" PropData=""/>
