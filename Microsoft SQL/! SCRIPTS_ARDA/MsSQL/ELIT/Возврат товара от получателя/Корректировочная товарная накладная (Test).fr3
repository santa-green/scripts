<TfrxReport Version="4.12" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.Author="sev12" ReportOptions.CreateDate="39762,7062155093" ReportOptions.Description.Text="" ReportOptions.LastChange="43727,4798581597" ScriptLanguage="PascalScript" ScriptText.Text=" &#13;&#10;procedure btn_StartOnClick(Sender: TfrxComponent);&#13;&#10;begin     &#13;&#10; dq_Doc.Open;&#13;&#10;  If dq_Doc.IsEmpty Then&#13;&#10;  Begin&#13;&#10;    MessageDlg('Нет данных для печати шапки документа.', mtError, mbOk, 0);&#13;&#10;    Engine.StopReport;&#13;&#10;  End;  &#13;&#10;  dq_DocD.Open;      &#13;&#10;  If dq_DocD.IsEmpty Then&#13;&#10;  Begin&#13;&#10;    //InputBox('info','RetChID', VarToStr(RetChID));&#13;&#10;    //InputBox('info','InvChID', VarToStr(InvChID));                              &#13;&#10;        &#13;&#10;    MessageDlg('Данные о корректировках отсутствуют! Проверьте связь между Расходной и Возвратной накладными по следующим полям:'#13 +&#13;&#10;      '&#34;Номер нал. источника&#34;, &#34;Дата&#34;, &#34;Фирма&#34;, &#34;Признак 2&#34;.', mtError, mbOk, 0);&#13;&#10;    Engine.StopReport;&#13;&#10;  End;        &#13;&#10;&#13;&#10;  Page1.Visible := (&#60;dq_DocD.&#34;CorReason&#34;&#62; = 'Коригування за кількістю') or (&#60;dq_DocD.&#34;CorReason&#34;&#62; = 'Повернення');&#13;&#10;  Page2.Visible := (&#60;dq_DocD.&#34;CorReason&#34;&#62; = 'Коригування за ціною');  &#13;&#10;  &#13;&#10;  If ctl_PrintFEAProdID.Checked Then&#13;&#10;  Begin&#13;&#10;  Memo38.Visible:=True;&#13;&#10;  Memo39.Visible:=True;&#13;&#10;  Memo40.Visible:=True;  &#13;&#10;  &#13;&#10;  Memo35.Visible:=True;&#13;&#10;  Memo36.Visible:=True;&#13;&#10;  Memo37.Visible:=True;&#13;&#10;  &#13;&#10;  END ELSE&#13;&#10;  Begin&#13;&#10;&#13;&#10;  Memo10113.Left := Memo10113.Left - Memo38.Width;  &#13;&#10;  Memo10113.Width :=Memo10113.Width + Memo38.Width;&#13;&#10;  Memo10136.Left := Memo10136.Left - Memo39.Width;&#13;&#10;  Memo10136.Width :=Memo10136.Width + Memo39.Width;&#13;&#10;  Memo1066.Left :=Memo1066.Left - Memo40.Width;&#13;&#10;  Memo1066.Width :=Memo1066.Width + Memo40.Width;  &#13;&#10;    &#13;&#10;  Memo5.Left := Memo5.Left - Memo38.Width;  &#13;&#10;  Memo5.Width := Memo5.Width + Memo38.Width;&#13;&#10;  Memo13.Left := Memo13.Left - Memo39.Width;&#13;&#10;  Memo13.Width := Memo13.Width + Memo39.Width;&#13;&#10;  Memo21.Left := Memo21.Left - Memo40.Width;&#13;&#10;  Memo21.Width := Memo21.Width + Memo40.Width;&#13;&#10;&#13;&#10;  End;&#13;&#10;&#13;&#10;    &#13;&#10;    &#13;&#10;  end;&#13;&#10; &#13;&#10;&#13;&#10;begin&#13;&#10;       &#13;&#10;  &#13;&#10;      &#13;&#10;end." StoreInDFM="False" PropData="">
