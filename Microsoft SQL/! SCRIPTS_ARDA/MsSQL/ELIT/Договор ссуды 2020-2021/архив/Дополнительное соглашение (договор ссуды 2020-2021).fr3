<TfrxReport Version="4.12" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="40862,573184919" ReportOptions.Description.Text="" ReportOptions.LastChange="43854,4952760301" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  RegPath: String = 'Бизнес\Отчёты\Доп соглашение ЮРЛИЦА';&#13;&#10;  CName: String;&#13;&#10;&#13;&#10;procedure btOkOnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  AFilter: String;&#13;&#10;begin&#13;&#10;  if (cb_Ours.Keyvalue = null) or ((cb_CompGrs2.Keyvalue = null) and (Trim(eComps.Text) = '')) or (cb_Emps.Keyvalue = null) then&#13;&#10;  begin&#13;&#10;    frmParams.ModalResult := mrNone;&#13;&#10;    MessageDlg('Если не знаете, с чего начать, заполните сперва поля &#34;Фирма&#34;, &#34;Группа предприятий&#34; и &#34;Представитель фирмы&#34;. Благодарим за внимание.', mtInformation, mbOk, 0);&#13;&#10;    Exit;&#13;&#10;  end;&#13;&#10;&#13;&#10;  Реестр_Записать('OurID', VarToStr(cb_Ours.KeyValue), RegPath);&#13;&#10;  //Реестр_Записать('eComps', eComps.Text, RegPath);&#13;&#10;  Реестр_Записать('CompGrID2', VarToStr(cb_CompGrs2.KeyValue), RegPath);&#13;&#10;  //Реестр_Записать('CompType', VarToStr(cb_CompTypes.KeyValue), RegPath);&#13;&#10;  Реестр_Записать('SRID', VarToStr(cbSRIDsA.KeyValue), RegPath);&#13;&#10;  Реестр_Записать('EmpID', VarToStr(cb_Emps.KeyValue), RegPath);      &#13;&#10;  dq_DocA.Open;&#13;&#10;&#13;&#10;  if dq_DocA.IsEmpty then&#13;&#10;  begin&#13;&#10;    //ShowMessage('dq_DocA.IsEmpty');                                                                      &#13;&#10;    dq_DocA.Close;&#13;&#10;    frmParams.ModalResult := mrNone;&#13;&#10;    MessageDlg('Нет данных для печати, проверьте установленные фильтры.', mtError, mbOk, 0);&#13;&#10;    Exit;&#13;&#10;  end;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure sbCompsOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10; {eComps.Text := выборспискаизсправочника('(SELECT CompID, CAST(CompID AS VARCHAR(25))+ CHAR(32) +' +&#13;&#10;    ' CHAR(45) + CHAR(32) + CompName CompName FROM r_Comps m WITH(NOLOCK) WHERE (CompGrID2 = ' + &#13;&#10;    dq_CompGrs2A.Fields[0].AsString + ' OR ' + VarToStr(IIF(cb_CompGrs2.KeyValue = null, 1, 0)) +  ' = 1) ' +&#13;&#10;    ' AND CompType = 1' + &#13;&#10;    ' AND EXISTS(SELECT * FROM dbo.at_z_Contracts WITH(NOLOCK) WHERE CompID=m.CompID AND ContrTypeID=1 AND Status=1 AND OurID=' + VarToStr(cb_Ours.KeyValue) + ')) m ORDER BY 1--;CompID;CompName;'); }  &#13;&#10;&#13;&#10; eComps.Text:=ВыборСпискаИзСправочника('r_Comps;CompID;CompName;'+eComps.Text);  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure frmParamsOnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  cb_Ours.keyvalue := Реестр_Прочитать('OurID', VarToStr(БДФункция('zf_GetOurID','t')), RegPath);&#13;&#10;  //eComps.Text := Реестр_Прочитать('eComps', '', RegPath);&#13;&#10;  cb_CompGrs2.KeyValue := Реестр_Прочитать('CompGrID2', '', RegPath);&#13;&#10;  //cb_CompTypes.KeyValue := Реестр_Прочитать('CompType', '', RegPath);&#13;&#10;  cbSRIDsA.KeyValue := Реестр_Прочитать('SRID', '', RegPath);&#13;&#10;  cb_Emps.KeyValue := Реестр_Прочитать('EmpID', '', RegPath);      &#13;&#10;  &#13;&#10;end;&#13;&#10;&#13;&#10;// 2019-12-27 rkv0 формируем порядок вывода данных (в свойствах дата-бендов убираем &#34;Формировать новую страницу&#34; и формируем новые страницы в коде).                                                                                                                                                                                                                                                                                                                         &#13;&#10;procedure Page1OnManualBuild(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  dq_DocA.Open;&#13;&#10;                                      &#13;&#10;  while not dq_DocA.Eof do&#13;&#10;    begin                                               &#13;&#10;      with Engine do      &#13;&#10;        begin&#13;&#10;          &#13;&#10;        //page1                       &#13;&#10;        if dq_DocA.RecNo &#62; 0 then NewPage;&#13;&#10;        ShowBand(MasterData1);&#13;&#10;        ShowBand(Child1);&#13;&#10;        ShowBand(Child2);&#13;&#10;        ShowBand(Child3);&#13;&#10;        //NewPage; //убрать галочку &#34;Формировать новую страницу&#34; на дата-бенде.                                                                    &#13;&#10;          &#13;&#10;        //page2          &#13;&#10;        {ShowBand(MasterData2);&#13;&#10;        ShowBand(Child5);&#13;&#10;        ShowBand(Header2);  &#13;&#10;&#13;&#10;        //выводим список CompAdd (адреса доставки) в доверенности.                                                                                                                                               &#13;&#10;        dq_DocD.Open;          &#13;&#10;        while not dq_DocD.Eof do&#13;&#10;          begin&#13;&#10;            ShowBand(DetailData2);&#13;&#10;            dq_DocD.Next;                                        &#13;&#10;          end;&#13;&#10;        dq_DocD.Close;}  &#13;&#10;          &#13;&#10;        //ShowBand(Footer1);&#13;&#10;        //ShowBand(Child6);&#13;&#10;                                              &#13;&#10;        end;&#13;&#10;    dq_DocA.Next;                  &#13;&#10;    end;&#13;&#10;    dq_DocA.Close;        &#13;&#10;end;&#13;&#10;&#13;&#10;procedure eCompsOnKeyPress(Sender: TfrxComponent; var Key: Char);&#13;&#10;begin&#13;&#10;   if ecomps.Text &#60;&#62; '' then cbSRIDsA.Enabled := False;  &#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." StoreInDFM="False" PropData="">
