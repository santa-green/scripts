<TfrxReport Version="4.12" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.OutlineVisible="True" PreviewOptions.Zoom="1" PrintOptions.Copies="2" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="40724,7112022569" ReportOptions.Description.Text="" ReportOptions.LastChange="43823,4870639468" ScriptLanguage="PascalScript" ScriptText.Text="Var&#13;&#10;  RegPath: String = 'Бизнес\Отчёты\Доверенность';&#13;&#10;  ArrPages: Variant;&#13;&#10;&#13;&#10;procedure sbCompsOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  eComps.Text := выборспискаизсправочника('(SELECT CompID, CAST(CompID AS VARCHAR(25))+ CHAR(32) +' +&#13;&#10;    ' CHAR(45) + CHAR(32) + CompName CompName FROM r_Comps m WITH(NOLOCK) WHERE (CompGrID2 = ' + &#13;&#10;    dq_CompGrs2.Fields[0].AsString + ' OR ' + VarToStr(IIF(cb_CompGrs2.KeyValue = null, 1, 0)) +  ' = 1) ' +&#13;&#10;    ' AND EXISTS(SELECT * FROM dbo.at_z_Contracts WITH(NOLOCK) WHERE CompID=m.CompID AND ContrTypeID=1 AND Status=1 AND OurID=' + VarToStr(cb_Ours.KeyValue) + ')) m ORDER BY 1--;CompID;CompName;');&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btOKOnClick(Sender: TfrxComponent);&#13;&#10;Var&#13;&#10;  AFilter: String;&#13;&#10;begin&#13;&#10;  if (cb_Ours.Keyvalue = null) or ((cb_CompGrs2.Keyvalue = null) and (Trim(eComps.Text) = '')) then&#13;&#10;  begin&#13;&#10;    frmParams.ModalResult := mrNone;      &#13;&#10;    MessageDlg('Поля &#34;Фирма&#34;, &#34;Группа предприятий&#34; обязательны для заполнения.', mtWarning, mbOk, 0);&#13;&#10;    Exit;&#13;&#10;  end;&#13;&#10;    &#13;&#10;  Реестр_Записать('OurID', VarToStr(cb_Ours.KeyValue), RegPath);&#13;&#10;  Реестр_Записать('eComps', eComps.Text, RegPath);&#13;&#10;  dq_Doc.Open;&#13;&#10;&#13;&#10;  If dq_Doc.IsEmpty Then&#13;&#10;  Begin&#13;&#10;    dq_Doc.Close;&#13;&#10;    MessageDlg('Нет данных для печати, проверьте установленные фильтры.', mtError, mbOk, 0);&#13;&#10;    frmParams.ModalResult := mrNone;&#13;&#10;  End;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure frmParamsOnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  cb_Ours.KeyValue := Реестр_Прочитать('OurID', VarToStr(БДФункция('zf_GetOurID','t')), RegPath);&#13;&#10;  eComps.Text := Реестр_Прочитать('eComps', '', RegPath);&#13;&#10;end;&#13;&#10;                 &#13;&#10;procedure DetailData1OnAfterPrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Header1.Visible := (DetailData1.DataSet.RecNo &#60;= DetailData1.DataSet.RecordCount - 1);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Header1.Visible := True;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." StoreInDFM="False" PropData="">
  <TfrxDataPage Name="Data" Height="256" Left="0" Top="0" Width="256">
    <TfrxBDEQuery Name="dq_CompGrs2" UserName="dq_CompGrs2" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="dq_Ours" IgnoreDupParams="True" SQL.Text="SELECT DISTINCT m.CompGrID2, CAST(CAST(m.CompGrID2 AS VARCHAR(10)) + ' - ' + m.CompGrName2 AS VARCHAR(255)) CompGrName2, :OurID OurID&#13;&#10;FROM r_CompGrs2 m WITH(NOLOCK)&#13;&#10;JOIN r_Comps d WITH(NOLOCK) ON d.CompGrID2 = m.CompGrID2&#13;&#10;WHERE (m.CompGrID2 BETWEEN 2030 AND 2099 OR m.CompGrID2 = 3)&#13;&#10;AND EXISTS(SELECT * FROM dbo.at_z_Contracts WITH(NOLOCK) WHERE CompID=d.CompID AND Status=1 AND ContrTypeID=1 AND OurID=:OurID)&#13;&#10;ORDER BY 1, 2" DatabaseName="dbMain" SessionName="Default" PropData="05704C65667402240470546F70023C0A506172616D657465727301010651204E616D653D224F75724944222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B64715F4F7572732E262333343B4F75724944262333343B262336323B220000"/>
    <TfrxBDEQuery Name="dq_Ours" UserName="dq_Ours" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT OurID, CAST(OurID AS VARCHAR(3)) + ' - ' + OurName OurName, Note2, Code&#13;&#10;FROM dbo.r_Ours&#13;&#10;WHERE OurID IN (SELECT AValue FROM dbo.af_GetValidsTable('ValidOurs'))&#13;&#10;ORDER BY 1" DatabaseName="dbMain" SessionName="Default" PropData="05704C65667402240470546F7002080A506172616D65746572730100"/>
    <TfrxBDEQuery Name="dq_Doc" UserName="dq_Doc" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="True" SQL.Text="SELECT rc.CompID, rc.Contract1, rc.Code, con.ContrID, con.BDate&#13;&#10;FROM dbo.r_Comps rc WITH(NOLOCK)&#13;&#10;LEFT JOIN dbo.at_r_CompOurTerms rcot WITH(NOLOCK) ON rcot.CompID = rc.CompID&#13;&#10;JOIN dbo.at_z_Contracts con WITH(NOLOCK) ON con.CompID = rc.CompID AND con.Status = 1 AND con.ContrTypeID = 1 AND con.OurID = rcot.OurID&#13;&#10;WHERE (:Comps = '' OR rc.CompID IN (SELECT AValue FROM dbo.af_FilterToTableByTable(:Comps, 'r_Comps')))&#13;&#10;AND rcot.OurID = :OurID&#13;&#10;AND rc.CompGrID2 BETWEEN 2030 AND 2099&#13;&#10;AND (:IsSRID = 1 OR rcot.SRID = :SRID)&#13;&#10;AND (:IsCompGrID2 = 1 OR rc.CompGrID2 = :CompGrID2)    &#13;&#10;ORDER BY 1" DatabaseName="dbMain" SessionName="Default" PropData=""/>
    <TfrxBDEQuery Name="dq_DocD" UserName="dq_DocD" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="dq_Doc" IgnoreDupParams="True" SQL.Text="SELECT rcadd.CompAdd&#13;&#10;FROM dbo.r_CompsADD rcadd WITH(NOLOCK)&#13;&#10;WHERE CompID = :CompID&#13;&#10;AND LEN(CompAdd) &#62; 4&#13;&#10;--moa0 '2019-09-27 17:01:05.332' Исключить адреса, у которых стоит Статус = 102. По заявке от Танцюры.    &#13;&#10;AND (SELECT TOP 1 StateCode FROM at_z_ContractsAdd WHERE CompID = rcadd.CompID AND CompAddID = rcadd.CompAddID ORDER BY ChID DESC) != 102&#13;&#10;ORDER BY CompAddID" DatabaseName="dbMain" SessionName="Default" PropData="05704C65667402780470546F70023C0A506172616D657465727301010652204E616D653D22436F6D704944222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B64715F446F632E262333343B436F6D704944262333343B262336323B220000"/>
    <TfrxBDEQuery Name="dq_SrIDs" UserName="dq_SRIDs" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="dq_CompGrs2" IgnoreDupParams="True" SQL.Text="SELECT DISTINCT rsr.SRID, CAST(CAST(rsr.SRID AS VARCHAR(10)) + ' - ' + SRName AS VARCHAR(250)) SRName&#13;&#10;FROM dbo.r_Comps rc WITH(NOLOCK)&#13;&#10;LEFT JOIN dbo.at_r_CompOurTerms rcot WITH(NOLOCK) ON rcot.CompID = rc.CompID&#13;&#10;JOIN dbo.at_r_SRS rsr WITH(NOLOCK) ON rcot.SRID = rsr.SRID&#13;&#10;WHERE (:IsCompGrID2 = 1 OR rc.CompGrID2 = :CompGrID2)&#13;&#10;AND rcot.OurID = :OurID&#13;&#10;AND EXISTS(SELECT * FROM dbo.at_z_Contracts WITH(NOLOCK) WHERE CompID=rc.CompID AND Status=1 AND ContrTypeID=1 AND OurID=:OurID)&#13;&#10;ORDER BY 1" DatabaseName="dbMain" SessionName="Default" PropData=""/>
