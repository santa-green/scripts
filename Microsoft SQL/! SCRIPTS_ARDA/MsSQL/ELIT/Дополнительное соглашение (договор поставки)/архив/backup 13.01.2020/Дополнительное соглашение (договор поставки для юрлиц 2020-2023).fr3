    <TfrxBDEQuery Name="dq_DocD" UserName="dq_DocD" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="True" SQL.Text="SELECT rcadd.CompAdd&#13;&#10;FROM dbo.r_CompsADD rcadd WITH(NOLOCK)&#13;&#10;WHERE CompID = :CompID&#13;&#10;AND LEN(CompAdd) &#62; 4&#13;&#10;--moa0 '2019-09-27 17:01:05.332' Исключить адреса, у которых стоит Статус = 102. По заявке от Танцюры.    &#13;&#10;AND (SELECT TOP 1 StateCode FROM at_z_ContractsAdd WHERE CompID = rcadd.CompID AND CompAddID = rcadd.CompAddID ORDER BY ChID DESC) != 102&#13;&#10;ORDER BY CompAddID" DatabaseName="dbMain" SessionName="Default" PropData="05704C65667403BC020470546F700398000A506172616D657465727301010653204E616D653D22436F6D704944222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B64715F446F63412E262333343B436F6D704944262333343B262336323B220000"/>
    <TfrxBDEQuery Name="dq_SrIDs" UserName="dq_SRIDs" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="True" SQL.Text="SELECT DISTINCT rsr.SRID, CAST(CAST(rsr.SRID AS VARCHAR(10)) + ' - ' + SRName AS VARCHAR(250)) SRName&#13;&#10;FROM dbo.r_Comps rc WITH(NOLOCK)&#13;&#10;LEFT JOIN dbo.at_r_CompOurTerms rcot WITH(NOLOCK) ON rcot.CompID = rc.CompID&#13;&#10;JOIN dbo.at_r_SRS rsr WITH(NOLOCK) ON rcot.SRID = rsr.SRID&#13;&#10;WHERE (:IsCompGrID2 = 1 OR rc.CompGrID2 = :CompGrID2)&#13;&#10;AND rcot.OurID = :OurID&#13;&#10;AND EXISTS(SELECT * FROM dbo.at_z_Contracts WITH(NOLOCK) WHERE CompID=rc.CompID AND Status=1 AND ContrTypeID=1 AND OurID=:OurID)&#13;&#10;ORDER BY 1" DatabaseName="dbMain" SessionName="Default" PropData=""/>
