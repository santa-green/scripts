    <TfrxBDEQuery Name="Query1" UserName="Query1" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT      &#13;&#10;      m.SrcPosID,    &#13;&#10;      m.BarCode,    &#13;&#10;      m.ProdID, p.UM, p.UAProdName as ProdName, p.Country,                   &#13;&#10;      mp.PriceMC*v.KursCC AS PriceCC, &#13;&#10;      CAST(FLOOR(mp.PriceMC*v.KursCC/10) as numeric(21,0)) AS PriceMoney,          &#13;&#10;      REVERSE(LEFT(REVERSE(CAST((mp.PriceMC*v.KursCC/10) AS NUMERIC(21,2))),2)) AS PricePenny,&#13;&#10;      CAST(FLOOR(mp.PromoPriceCC/10) as numeric(21,0)) AS NewPriceMoney,               &#13;&#10;      REVERSE(LEFT(REVERSE(CAST((mp.PromoPriceCC/10) AS NUMERIC(21,2))),2)) AS NewPricePenny      &#13;&#10;     &#13;&#10;      FROM iv_PrintPriceList m   &#13;&#10;      INNER JOIN dbo.r_Prods p  ON  p.ProdID=m.ProdID&#13;&#10;      INNER JOIN r_ProdMP mp ON mp.ProdID = p.ProdID AND mp.PLID =	70	&#13;&#10;      INNER JOIN r_ProdMQ mq ON mq.ProdID = p.ProdID AND mq.Um = p.Um &#13;&#10;      INNER JOIN r_Currs v ON v.CurrID=mp.CurrID&#13;&#10;      WHERE mp.PromoPriceCC &#62; 0 AND mp.PromoPriceCC &#60; mp.PriceMC AND dbo.zf_GetDate(GETDATE()) BETWEEN (mp.BDate - 3) AND mp.EDate AND mp.InUse = 1    &#13;&#10;      ORDER BY  m.SrcPosID  " DatabaseName="dbMain" SessionName="Default" PropData="05704C656674022C0470546F7002300A506172616D65746572730100"/>
